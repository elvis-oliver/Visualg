algoritmo "SOMA COM FUNÇÃO"
var
   V1, V2, S: inteiro
   funcao Soma (A, B: inteiro) : inteiro
   inicio
   retorne A + B
   fimfuncao
inicio
      escreva ("digite o primeiro valor: ")
      leia (V1)
      escreva ("digite o segundo valor: ")
      leia (V2)
      S <- Soma (V1, V2)
      escreva ("a soma entre ", V1," e ", V2," é igual a: ", S)

fimalgoritmo